<?xml version="1.0" encoding="UTF-8"?>

<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">
  <component>
    <component-type>UICasesBPMProcessMgmt</component-type>
    <component-class>is.idega.idegaweb.egov.bpm.cases.presentation.UICasesBPMProcessMgmt</component-class>
    <component-extension>
      <idegaweb-info>
        <builder-visible>true</builder-visible>
        <object-type>jsf.uicomponent</object-type>
      </idegaweb-info>
    </component-extension>
  </component>
  <component>
    <component-type>com.idega.UICasesBPMAssets</component-type>
    <component-class>is.idega.idegaweb.egov.bpm.cases.presentation.UICasesBPMAssets</component-class>
    <component-extension>
      <idegaweb-info>
        <builder-visible>true</builder-visible>
        <object-type>jsf.uicomponent</object-type>
      </idegaweb-info>
    </component-extension>
  </component>
  <component>
  	<component-type>com.idega.UICasesListAsset</component-type>
  	<component-class>is.idega.idegaweb.egov.bpm.cases.presentation.UICasesListAsset</component-class>
	<property>
		<display-name>Case ID</display-name>
		<property-name>caseId</property-name>
		<property-class>java.lang.Integer</property-class>
	</property>
	<property>
		<display-name>Allow download document as PDF file</display-name>
		<property-name>downloadDocument</property-name>
		<property-class>boolean</property-class>
	</property>
  	<component-extension>
  		<idegaweb-info>
			<builder-visible>true</builder-visible>
			<object-type>jsf.uicomponent</object-type>
  		</idegaweb-info>
  	</component-extension>
  </component>
</faces-config>